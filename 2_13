#include <iostream>
using namespace std;
#include <cmath>;


int main()
{
int a,b,c;
double d,x1,x2;
cout << "Введите коэфициенты a, b, c: ";
cin >> a >> b >> c;
d=b*b-(4*a*c);

if (d>=0){
int x1 = ((-b + sqrt(d))/(2*a));
int x2 = ((-b - sqrt(d))/(2*a));
cout << "Корни уравнения:";
cout << endl;

if (x1==x2){
cout << "x = "<< x1;
}
if (x1!=x2) {
cout << "x1 = "<< x1;
cout << endl;
cout << "x2 = "<< x2;
}
}
    
if (d<0) {
cout << "Корней нет";
}
    return 0;
}
